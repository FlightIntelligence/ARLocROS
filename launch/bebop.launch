<launch>
    <node pkg="rosjava_catkin_package_a" type="ARLocROS" name="ARLocROS" args="com.github.rosjava_catkin_package_a.ARLocROS.ARLoc" output="screen">
        <param name="pattern_dir" value="$(find rosjava_catkin_package_a)/ARLocROS/res/"/>
        <param name="marker_frame_name" value="marker" />
        <param name="camera_frame_name" value="camera_optical" />
        <param name="camera_image_topic" value="/bebop/image_raw" />
        <param name="camera_info_topic" value="/bebop/camera_info" />
        <param name="marker_config_file" value="$(find rosjava_catkin_package_a)/ARLocROS/config/marker.cfg" />
    </node>
</launch>
